{
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "create a auto updating bookmark"
  },
  "description": "create auto updating bookmarks in the same domain scope",
  "icons": {
    "250": "icon.png"
  },
  "manifest_version": 2,
  "name": "Auto Updating Bookmark",
  "permissions": [
    "tabs",
    "bookmarks",
    "storage", 
    "menus"
  ],
  "version": "1.5.3"
}
