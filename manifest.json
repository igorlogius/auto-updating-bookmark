{
  "author": "igorlogius",
  "homepage_url": "https://github.com/igorlogius/auto-updating-bookmark",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_title": "create a auto updating bookmark"
  },
  "icons": {
    "250": "icon.png"
  },
  "manifest_version": 2,
  "name": "Auto Updating Bookmark",
  "permissions": [
    "tabs",
    "bookmarks",
    "storage", 
    "menus"
  ],
  "version": "1.6.6"
}
